{UTF-8}
Adding=
	Employees
	~	person*years/year
	~		|

Average Salary=
	Payroll/Employees
	~	$/(year*person)
	~		|

Average Tenure=
	Tenure/Employees
	~	year
	~		|

Bidding=
	Salary On Offer*Salary Pressure/Time to Adjust Offer
	~	$/(year*person)/year
	~		|

Desired Hiring Rate=
	Vacancies/Time to Hire+"Firing + Losing"
	~	people/year
	~		|

Desired Switching Rate=
	Structural Switching Rate+Normal Switching Rate*(MAX(0,Switching Spread-Friction Spread\
		)/Normal Spread)^Switching Sensitivity
	~	fraction/year
	~		|

Employees= INTEG (
	Hiring-"Firing + Losing"-Moving+Moving,
		Jobs*(1-Initial Vacancy Rate))
	~	people
	~		|

"Firing + Losing"=
	Employees*Structural Turnover Rate
	~	people/year
	~		|

Friction Spread=
	0.03
	~	fraction
	~		|

Growing=
	Jobs*(STEP(Growth Rate + RANDOM NORMAL(-6,6,0,1,0)*Noise SD, Test Time) + Step Size*\
		PULSE(Test Time,0)/TIME STEP)
	~	people/year
	~		|

Growth Rate=
	0
	~	fraction/year [-0.1,0.1]
	~		|

Hiring=
	MIN(Desired Hiring Rate,Job Seekers/Time to Hire)
	~	people/year
	~		|

Initial Salary=
	100000
	~	$/(year*person)
	~		|

Initial Unemployment Rate=
	0.05
	~	fraction
	~		|

Initial Vacancy Rate=
	0.05
	~	fraction
	~		|

Job Seekers= INTEG (
	Net Entering+"Firing + Losing"-Hiring,
		Employees*Initial Unemployment Rate/(1-Initial Unemployment Rate))
	~	people
	~		|

Jobs= INTEG (
	Growing,
		1e+006)
	~	people
	~		|

Little's Law Tenure=
	1/Total Turnover Rate
	~	year
	~		|

Losing=
	Tenure*Total Turnover Rate
	~	person*years/year
	~		|

Moving=
	Employees*Desired Switching Rate
	~	people/year
	~		|

Net Entering=
	(Employees+Job Seekers)*Net Entry Rate
	~	people/year
	~		|

Net Entry Rate=
	Sector Attractiveness*Sector Wage Sensitivity/Sector Response Time
	~	fraction/year
	~		|

Noise SD=
	0
	~	fraction/year [0,0.1]
	~		|

Normal Spread=
	0.1
	~	fraction
	~		|

Normal Switching Rate=
	0.05
	~	fraction/year
	~		|

Parity Adjustment Rate=
	Switching Spread/Time to Adjust Salary
	~	fraction/year
	~		|

Payroll= INTEG (
	Raising+Setting,
		Initial Salary*Employees)
	~	$/year
	~		|

Raising=
	Payroll*Parity Adjustment Rate
	~	$/year/year
	~		|

Salary On Offer= INTEG (
	Bidding,
		Initial Salary)
	~	$/(year*person)
	~		|

Salary Pressure=
	(Vacancy Rate-Initial Vacancy Rate) - (Unemployment Rate-Initial Unemployment Rate)
	~	dmnl
	~		|

Sector Attractiveness=
	Salary On Offer/Initial Salary-1
	~	dmnl
	~		|

Sector Response Time=
	4
	~	year [1,10]
	~		|

Sector Wage Sensitivity=
	1
	~	dmnl
	~		|

Setting=
	Salary On Offer*(Moving+Hiring) - Average Salary*(Moving+"Firing + Losing")
	~	$/year/year
	~		|

Step Size=
	0
	~	fraction [0,0.1]
	~		|

Structural Switching Rate=
	0.05
	~	fraction/year
	~		|

Structural Turnover Rate= INITIAL(
	Initial Vacancy Rate/Time to Hire)
	~	fraction/year
	~		|

Switching Sensitivity=
	2
	~	dmnl
	~		|

Switching Spread=
	Salary On Offer/Average Salary-1
	~	fraction
	~		|

Tenure= INTEG (
	Adding-Losing,
		Employees/Total Turnover Rate)
	~	person*years
	~		|

Test Time=
	1
	~	year
	~		|

Time to Adjust Offer=
	0.5
	~	year
	~		|

Time to Adjust Salary=
	1
	~	year [0.25,5]
	~		|

Time to Hire=
	0.5
	~	year
	~		|

Total Turnover Rate=
	Desired Switching Rate+Structural Turnover Rate
	~	fraction/year
	~		|

Unemployment Rate=
	Job Seekers/(Job Seekers+Employees)
	~	fraction
	~		|

Vacancies=
	Jobs-Employees
	~	people
	~		|

Vacancy Rate=
	Vacancies/Jobs
	~	fraction
	~		|

